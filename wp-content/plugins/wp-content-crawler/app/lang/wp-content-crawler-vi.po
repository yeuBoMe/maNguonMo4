#: storage/cache/c8d7d4467434b48703ea66b611e76ba806ca54c2.php:128
#: storage/cache/d2e4ee0831305dc7b3272ca0e38a89a012e680d2.php:148
msgid "script"
msgstr "kịch bản"

#: storage/cache/c8d7d4467434b48703ea66b611e76ba806ca54c2.php:129
#: storage/cache/d2e4ee0831305dc7b3272ca0e38a89a012e680d2.php:149
